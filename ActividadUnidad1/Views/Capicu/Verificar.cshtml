@{
    ViewBag.Title = "Verificar Número Capicú";
}

<h2>Verificar Número Capicú</h2>

@if (ViewBag.EsCapicu)
{
    <p>El número @ViewBag.Numero es un número Capicú.</p>
}
else
{
    <p>El número @ViewBag.Numero no es un número Capicú.</p>
}

