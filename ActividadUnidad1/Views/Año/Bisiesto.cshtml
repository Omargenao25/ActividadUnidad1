@{
    ViewBag.Title = "Verificar Año Bisiesto";
}

<h2>Verificar Año Bisiesto</h2>

@if (ViewBag.EsBisiesto)
{
    <p>El año @ViewBag.Año es un año bisiesto.</p>
}
else
{
    <p>El año @ViewBag.Año no es un año bisiesto.</p>
}

