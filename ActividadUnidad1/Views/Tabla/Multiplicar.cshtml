@{
    ViewBag.Title = "Tabla de Multiplicar";
}

<h2>Tabla de Multiplicar del @ViewBag.Numero</h2>

<ul>
    @foreach (var item in ViewBag.TablaMultiplicar)
    {
        <li>@item</li>
    }
</ul>
